{
  "version": "2.0.0",
  "runtimeConfiguration": {
      "environment": "default",
      "logLevel": "info",
      "logDestination": "stdout+platform",
      "logMaxFileSize": 2147483648,
      "requestHeaderNameRequestId": "x-scan-request-id",
      "requestHeaderNameScenarioId": "x-scan-scenario-id",
      "requestFlowrate": 10,
      "requestTimeout": 30,
      "requestTlsInsecureSkipVerify": true,
      "responseFollowRedirection": true,
      "happyPathOnly": false,
      "maxScanDuration": 3600,
      "memoryLimit": 2147483648,
      "memoryTimeSpan": 10,
      "responseMaxBodySizeScan": 10485760,
      "reportMaxHttpResponseSizeHappyPath": 8092,
      "reportMaxBodySizeHappyPath": 8092,
      "reportMaxHttpResponseSizeTest": 8092,
      "reportMaxBodySizeTest": 8092,
      "reportIssuesOnly": true,
      "reportMaxIssues": 10000,
      "reportMaxSize": 104857600
  },
  "customizations": {
      "happyPaths": {
          "retry": 3,
          "responsePolicy": {
              "httpStatusExpected": true,
              "mustBeConformant": true
          },
          "httpStatusExpected": [
              "2XX",
              "400",
              "404"
          ]
      },
      "tests": {
          "responsePolicy": {
              "httpStatusExpected": true,
              "mustBeConformant": true
          },
          "responseAnalysisBehavior": {
              "path-item-method-not-allowed-scan": {
                  "httpStatusExpected": [
                      "400"
                  ]
              },
              "authentication-swapping-bola": {
                  "httpStatusExpected": [
                      "401",
                      "403"
                  ]
              },
              "authentication-swapping-bfla": {
                  "httpStatusExpected": [
                      "401",
                      "403"
                  ]
              }
          }
      }
  },
  "authorizationTests": {
      "UserIsolation": {
          "key": "authentication-swapping-bola",
          "source": [
              "UserCookie/User"
          ],
          "target": [
              "UserCookie/User1"
          ]
      },
      "AdminOnly": {
          "key": "authentication-swapping-bfla",
          "source": [
              "AdminCookie/Admin"
          ],
          "target": [
              "UserCookie/User"
          ]
      }
  },
  "authenticationDetails": [
      {
          "AdminCookie": {
              "type": "apiKey",
              "in": "cookie",
              "name": "session",
              "default": "Admin",
              "ttl": "30s",
              "credentials": {
                  "Admin": {
                      "description": "Admin Credential",
                      "requests": [
                          {
                              "environment": {
                                  "password": "HelloWorld",
                                  "user": "admin@42crunch.com"
                              },
                              "$ref": "#/operations/UserLogin/request",
                              "auth": []
                          }
                      ],
                      "credential": "{{sessionId}}"
                  }
              }
          },
          "UserCookie": {
              "type": "apiKey",
              "in": "cookie",
              "name": "session",
              "default": "User",
              "ttl": "30s",
              "credentials": {
                  "User": {
                      "description": "User Credential",
                      "requests": [
                          {
                              "environment": {
                                  "password": "HelloWorld",
                                  "user": "user@42crunch.com"
